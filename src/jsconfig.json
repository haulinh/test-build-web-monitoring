{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "experimentalDecorators": true,
        "paths": {
          "components/*": ["src/components/*"],
          "utils/*": ["src/utils/*"],
          "redux/*": ["src/redux/*"],
          "routes/*": ["src/routes/*"],
          "api/*": ["src/api/*"]
        }
    }
}